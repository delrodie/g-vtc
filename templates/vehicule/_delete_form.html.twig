<form method="post" action="{{ path('app_vehicule_delete', {'id': vehicule.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ce vehicule?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vehicule.id) }}">
    <button class="btn btn-danger btn-sm"><i class="bi bi-trash3"></i> Supprimer</button>
</form>
